{
  "name": "js-antlr",
  "version": "1.0.0",
  "description": "Regular parser for any antlr grammar",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start": "npx webpack serve",
    "watch": "webpack --watch",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --fix",
    "grammar": "antlr4 -Dlanguage=JavaScript -visitor -listener ./src/grammar/Decaf.g4 -o ./src/grammar/"
  },
  "keywords": [
    "antlr",
    "javascript"
  ],
  "author": "Oscar Ju√°rez",
  "license": "MIT",
  "dependencies": {
    "ace-builds": "^1.4.12",
    "antlr4": "^4.9.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "serve": "^12.0.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
