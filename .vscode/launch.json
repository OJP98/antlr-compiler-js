  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch webpack",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/webpack/bin/webpack.js",
      "args": [
        "serve",
        "--progress",
        "--config",
        "./webpack.config.js"
      ],
      "autoAttachChildProcesses": true,
      "url": "http://localhost:5000/",
      "webRoot": "${workspaceRoot}/src",
    }
  ]
}