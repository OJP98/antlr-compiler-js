  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Webpack",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/webpack-cli",
      "args": [
        "--config",
        "webpack.config.js"
      ],
      "autoAttachChildProcesses": true,
      "stopOnEntry": true,
      "url": "http://localhost:5000/",
      "webRoot": "${workspaceRoot}/src",
      "sourceMaps": true,
    }
  ]
}